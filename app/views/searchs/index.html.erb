<%= render "share/products_sidebar" %>
<div class="zy_right">
  <%= render "share/product_position"%>
  <div class="fdc_con">
    <div class="fd_top">为您查找到以下轮胎产品：</div>
    <% unless @products.first.blank? %>
        <%=render @products %>
    <% else %>
        <p>无返回结果.</p>
    <% end %>
    <div class="fd_btn"><input type="button" onclick="window.location='/products/search'" class="back_btn" value="返回重新查找" /></div>
  </div>
</div>


 
